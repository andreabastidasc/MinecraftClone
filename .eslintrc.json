{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      // Reglas personalizadas aqu√≠
  
      // Ejemplo: Permitir consolas en el entorno de desarrollo
      "no-console": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  